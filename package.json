{
  "name": "obs-studio-node",
  "version": "1.0.0",
  "description": "Experimental bindings to obs-studio using nan directly",
  "main": "index.js",
  "scripts": {
    "docs": "tsc build_docs.ts && node build_docs.js",
    "install": "cmake-js configure --debug && cmake --build build --target install",
    "test": "tsc --project test && ava --serial"
