  "cmake-js": {
    "runtime": "node",
    "runtimeVersion": "7.4.0",
    "arch": "x64"
  },
  "devDependencies": {
    "ava": "^0.20.0",
    "cmake-js": "^3.4.1",
    "gulp": "^3.9.1",
    "gulp-typedoc": "^2.0.2",
    "nan": "^2.6.2",
    "run-sequence": "^2.0.0",
    "typedoc": "^0.7.2",
    "typescript": "^2.4.1"
  },
  "dependencies": {
    "electron": "1.6.10",
    "electron-rebuild": "^1.5.11"
  }
